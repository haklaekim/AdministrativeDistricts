<rdf:Description rdf:about="http://lod.datahub.kr/id/cf/Donhwamun-gate--seoul-intangible-cultural-heritage-exhibition-hall">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
