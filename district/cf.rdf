<rdf:Description rdf:about="http://lod.datahub.kr/id/cf/Korea-university-museum">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
